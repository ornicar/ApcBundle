{
    "name": "ornicar/apc-bundle",
    "type":        "symfony-bundle",
    "description": "Provides a command line task to clear APC cache from the console",
    "keywords":    ["apc", "task"],
    "homepage":    "https://github.com/ornicar/ApcBundle",
    "license":     "MIT",
    "authors": [
        {
            "name": "Thibault Duplessis",
            "homepage": "https://github.com/ornicar"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/ornicar/ApcBundle/contributors"
        }
    ],
    "require": {
        "symfony/framework-bundle": ">=2.1,<3.0"
    },
    "suggest": {
        "fabpot/goutte": "To extract data from html/xml responses"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "symfony/console": ">=2.0,<3.0",
        "fabpot/goutte": "1.*"
    },
    "extra": {
        "branch-alias": {
            "dev-1.0": "1.0.x-dev",
            "dev-master": "1.1.x-dev"
        }
    },
    "autoload": {
        "psr-0": {
            "Ornicar\\ApcBundle": ""
        }
    },
    "target-dir": "Ornicar/ApcBundle"
}
